# Supabase (local defaults from `supabase start`)
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# Postgres (direct connection to Supabase local Postgres)
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:54322/postgres

# Redis — password must match --requirepass in docker-compose.yml
REDIS_PASSWORD=changeme
REDIS_URL=redis://:changeme@localhost:6379/0

# GitHub App
GITHUB_APP_ID=
GITHUB_PRIVATE_KEY=
GITHUB_WEBHOOK_SECRET=

# LLM Provider API Keys (at least one required for agent runs)
# Default provider: anthropic / claude-sonnet-4-5
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=

# Observability — leave blank to disable
SENTRY_DSN=

# App
DEBUG=true
CORS_ORIGINS=["*"]
